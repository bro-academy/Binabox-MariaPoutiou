<form class="catalog-search" method="get">
  {% include "partials/dropdown.njk" %}
  <input
    class="input catalog__search-bar"
    type="search"
    name="{{ catalogSearch.name }}"
    maxlength="{{ catalogSearch.maxlength }}"
    placeholder="{{ catalogSearch.placeholder }}"
    autocomplete="{{ catalogSearch.autocomplete }}"
  />
</form>
