 <footer class="footer container">
      <div class="footer-upper">
        <div class="footer-logo">
          <a class="footer-logo__link" href="{{ footer.logolink }}" title="{{ footer.logotitle }}">
            <img class="footer-logo__img" src="{{ footer.logoimg }}" alt="{{ footer.logoalt }}" loading="lazy" decoding="async"/>
          </a>
        </div>
         <p class="footer__description">{{ footer.description }}</p>
        <div class="footer__list-wrapper">
          {% for nav in footerNav %}
          <nav class="footer-nav">
            <h5 class="footer-nav__title">{{ nav.title }}</h5>
            <ul class="footer-nav__list">
              {% for link in nav.links %}
              <li class="footer-nav__list-item">
                <a class="footer-nav__link" href="{{ link.url }}">{{ link.item }}</a>
              </li>
              {% endfor %}
            </ul>
          </nav>
          {% endfor %}
       </div>
      </div>
      <div class="footer-lower">
        <div class="copyright">
          <p class="copyright__text">{{ footer.copyright }}</p>
        </div>
          <ul class="social-media-list social-media-list--footer">
            {% for social in footerSocials %}
            <li class="social-media__list-item">
              <a class="social-media__link" href="{{ social.link }}" target="_blank">
                <img class="social-media__icon" src="{{ social.img }}" alt="{{ social.alt }}" loading="lazy" decoding="async"/> 
              </a>
            </li>
            {% endfor %}
          </ul>
        </div>
    </footer>