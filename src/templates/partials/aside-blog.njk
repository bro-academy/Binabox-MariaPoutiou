<section class="aside__block">
  <h3 class="aside__block-title">{{ asideUpper.title }}</h3>
  <nav class="aside__block-nav">
    <ul class="aside__block-list">
      {% for item in asideUpper.item %}
      <li class="aside__block-item">
        <a class="aside__block-link" href="{{ item.link }}">{{ item.text }}</a>
      </li>
      {% endfor %}
    </ul>
  </nav>
</section>
<section class="aside__block aside__block--lower">
  <h4 class="aside__block-title">{{ asideLower.title }}</h4>
  <ul class="tag-list tag-list--aside">
    {% for item in asideLower.item %}
    <li class="tag-list__item">
      <a class="tag-list__link" href="{{ item.link }}">{{ item.text }}</a>
    </li>
    {% endfor %}
  </ul>
</section>
